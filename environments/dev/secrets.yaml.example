# Secrets Configuration - Development
# Copy this to secrets.yaml and encrypt with SOPS:
# cp secrets.yaml.example secrets.yaml
# sops -e -i secrets.yaml

cognito:
  # Client secret from Cognito app client
  client_secret: "your-cognito-client-secret-here"

github:
  # Personal access token for private repositories (optional)
  token: ""

database:
  # If using RDS for JupyterHub state (optional)
  password: ""

smtp:
  # Email configuration (optional)
  username: ""
  password: ""

datadog:
  # Monitoring API key (optional)
  api_key: ""

slack:
  # Webhook for notifications (optional)
  webhook_url: ""