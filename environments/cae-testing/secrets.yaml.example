# CAE Development Secrets Template
# Copy this to secrets.yaml and encrypt with SOPS
#
# To encrypt:
#   sops --encrypt --kms <KMS_KEY_ARN> secrets.yaml.example > secrets.yaml
#
# The Cognito client secret can be found in AWS Console:
#   1. Go to Amazon Cognito > User Pools > cae
#   2. App integration > App client list
#   3. Click on the app client (ID: 3jesa7vt6hanjscanmj93cj2kg)
#   4. Show client secret
#
# NOTE: cae-dev uses the SAME Cognito user pool as cae production
# This ensures identical authentication behavior for testing

cognito:
    client_secret: "YOUR_COGNITO_CLIENT_SECRET_HERE"

# GitHub OAuth (not used - using Cognito)
github:
    client_id: ""
    client_secret: ""
    token: ""
    org_whitelist: ""

# Optional integrations (not currently used)
database:
    password: ""
smtp:
    username: ""
    password: ""
datadog:
    api_key: ""
slack:
    webhook_url: ""
